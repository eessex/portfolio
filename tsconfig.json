{
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": "./src",
    "experimentalDecorators": true,
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "lib": ["es2016", "dom"],
    "moduleResolution": "node",
    "noEmit": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "./public/",
    "pretty": true,
  }
}
